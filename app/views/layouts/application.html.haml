!!!
%html
  %head
    %title Practplan
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}
    = csrf_meta_tags
    = stylesheet_link_tag 'application', :media => 'all'
    = javascript_include_tag 'application'
  %body
    .container-fluid
      = render 'navbar'
      .container.content.max-width
        = render 'flash'
        = yield
      = render 'footer'
